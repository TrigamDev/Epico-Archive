{
    "compilerOptions": {
        "target": "ES6",
        "module": "ES2022",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "moduleResolution": "Node",
        "allowImportingTsExtensions": true,
        "noEmit": true
    }
}
